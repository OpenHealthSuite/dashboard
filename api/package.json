{
    "name": "paceme-api",
    "version": "0.7.2",
    "description": "The API Behind PaceMe, for serving the web application",
    "main": "dist/index.js",
    "scripts": {
        "test": "jest",
        "build": "tsc",
        "migrations:validate": "pg-validate-migrations 'migrations'",
        "start": "node .",
        "start:headless": "node dist/headless.js",
        "start:dev": "concurrently 'node -r dotenv/config dist/index.js' 'node -r dotenv/config dist/headless.js'",
        "start:watch": "concurrently 'npm run build -- --watch' 'nodemon -r dotenv/config dist/index.js' 'nodemon -r dotenv/config dist/headless.js'",
        "lint": "npx eslint . --ext .ts",
        "dev:spinup": "scripts/dev_prestart.sh",
        "dev:teardown": "scripts/dev_teardown.sh",
        "dev:generateenv": "scripts/localenvgen.sh",
    },
    "author": "Lee Martin <hello@leejohnmartin.co.uk>",
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/ioredis": "^4.28.7",
        "@types/jest": "^27.0.3",
        "@types/lodash": "^4.14.177",
        "@types/node": "^16.11.9",
        "@types/pg": "^8.6.1",
        "@types/pg-format": "^1.0.2",
        "@types/sinon": "^10.0.6",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "concurrently": "^7.0.0",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jest": "^25.2.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.1.1",
        "jest": "^27.3.1",
        "lodash": "^4.17.21",
        "nodemon": "^2.0.15",
        "sinon": "^12.0.1",
        "source-map-support": "^0.5.21",
        "ts-jest": "^27.0.7",
        "ts-sinon": "^2.0.2",
        "typescript": "^4.5.2"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "ioredis": "^4.28.3",
        "neverthrow": "^4.3.1",
        "pg": "^8.7.3",
        "postgres-migrations": "^5.3.0"
    }
}
