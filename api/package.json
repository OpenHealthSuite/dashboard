{
  "name": "paceme-api",
  "version": "0.7.2",
  "description": "The API Behind PaceMe, for serving the web application",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node .",
    "start:headless": "node dist/headless.js",
    "start:dev:api": "nodemon -r dotenv/config src/index.ts",
    "start:dev:headless": "nodemon -r dotenv/config src/headless.ts",
    "start:dev": "concurrently 'npm run start:dev:api' 'npm run start:dev:headless'",
    "lint": "npx eslint . --ext .ts",
    "dev:spinup": "scripts/dev_prestart.sh",
    "dev:teardown": "scripts/dev_teardown.sh",
    "dev:generateenv": "scripts/localenvgen.sh"
  },
  "author": "Lee Martin <hello@leejohnmartin.co.uk>",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.3",
    "lodash": "^4.17.21",
    "nodemon": "^3.0.3",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-sinon": "^2.0.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cassandra-driver": "^4.6.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "^5.3.1",
    "neverthrow": "^6.0.0"
  }
}
